{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "url-util",
  "title": "URL Util",
  "description": "Detect the URL and crawl the content inside to display it. It also displays the information of the Query and supports editing",
  "icon": "command-icon.png",
  "author": "thoamsy yang",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "URL Parse",
      "description": "Template with a plain detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.1",
    "cheerio": "^1.0.0-rc.10",
    "node-fetch": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
